+    zsock_destroy(&s->hdl_resp);
+    zsock_destroy(&s->qemu_req);
+    zsock_destroy(&s->qemu_resp);
+    zsock_destroy(&s->nic_req);
+    zsock_destroy(&s->nic_resp);
+    zsock_destroy(&ls_sock);
+}
